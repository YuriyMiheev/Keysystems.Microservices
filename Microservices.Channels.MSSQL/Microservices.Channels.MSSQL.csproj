<Project Sdk="Microsoft.NET.Sdk.Web">

  <PropertyGroup>
    <TargetFramework>netcoreapp3.0</TargetFramework>
  </PropertyGroup>

  <ItemGroup>
    <Compile Remove="CommonHub.cs" />
    <Compile Remove="src\Configuration\IChannelConfigFileSettings.cs" />
    <Compile Remove="src\Configuration\IServiceConfig.cs" />
    <Compile Remove="src\Configuration\IServiceConfigFileSettings.cs" />
    <Compile Remove="src\Configuration\ServiceConfig.cs" />
    <Compile Remove="src\Controllers\CommonHub.cs" />
    <Compile Remove="src\SendMessageProcessor.cs" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.AspNetCore.SignalR" Version="1.1.0" />
    <PackageReference Include="Microsoft.AspNetCore.SignalR.Protocols.MessagePack" Version="3.0.0" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\Microservices.Channels\Microservices.Channels.csproj" />
  </ItemGroup>

  <ItemGroup>
    <Compile Update="channel.Designer.cs">
      <DesignTimeSharedInput>True</DesignTimeSharedInput>
      <AutoGen>True</AutoGen>
      <DependentUpon>channel.config</DependentUpon>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <Content Update="channel.config">
      <Generator>SettingsSingleFileGenerator</Generator>
      <LastGenOutput>channel.Designer.cs</LastGenOutput>
    </Content>
  </ItemGroup>

  <ItemGroup>
    <Folder Include="src\Configuration\" />
  </ItemGroup>

  <ProjectExtensions><VisualStudio><UserProperties channel_1json__JsonSchema="http://json-schema.org/draft-04/schema" /></VisualStudio></ProjectExtensions>

</Project>
